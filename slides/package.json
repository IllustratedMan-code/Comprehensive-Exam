{
  "name": "slides",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@fireworks-js/vue": "^2.10.8",
    "@slidev/cli": "^51.6.0",
    "@slidev/theme-apple-basic": "^0.25.1",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "prettier": {
    "overrides": [
      {
        "files": ["slides.md", "pages/*.md"],
        "options": {
          "parser": "slidev",
          "plugins": ["prettier-plugin-slidev"]
        }
      }
    ]
  }
}
